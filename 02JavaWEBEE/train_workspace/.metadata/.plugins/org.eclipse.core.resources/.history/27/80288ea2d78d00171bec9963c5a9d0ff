package com.eems.entity;
  
import com.eems.entity.Equipment;  
  
public class CartItem {  
    private Equipment equip;  
  
    private int count;  
  
    public int getCount() {  
        return count;  
    }  
  
    public void setCount(int count) {  
        this.count = count;  
    }

	public Equipment getEquip() {
		return equip;
	}

	public void setEquip(Equipment equip) {
		this.equip = equip;
	}  
  
}  
