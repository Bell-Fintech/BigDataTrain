package zd.service.impl;

import java.util.List;

import org.apache.ibatis.session.SqlSession;

import cn.edu360.mybatis.dao.OrdersMapper;
import cn.edu360.mybatis.model.Orders;
import cn.edu360.mybatis.service.OrdersService;
import cn.edu360.mybatis.util.MybatisUtil;

public class OrdersServiceImpl implements OrdersService {

	@Override
	public List<Orders> findAllOrdersAndItemsAndPost() {
		SqlSession session=MybatisUtil.getFactory().openSession();
		OrdersMapper daili=session.getMapper(OrdersMapper.class);
		return daili.findAllOrdersAndItemsAndPost();
	}

}
