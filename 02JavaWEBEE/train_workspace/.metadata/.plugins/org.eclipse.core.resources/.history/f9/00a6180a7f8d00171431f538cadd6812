package com.eems.entity;
  
import com.eems.entity.Equipment;  
  
public class CartItem {  
    private Equipment equip;  
  
    private int count;  
  
    public int getCount() {  
        return count;  
    }  
  
    public void setCount(int count) {  
        this.count = count;  
    }  
  
    public Equipment getProduct() {  
        return equip;  
    }  
  
    public void setProduct(Equipment product) {  
        this.equip = product;  
    }  
}  
