import urllib
import urlparse
#dictionary-->url ascii
def urlencode():
    params={'socre':100,'name':'spide_basic','comment':'good'}
    qs=urllib.urlencode(params)
    print(qs)
    print(urlparse.parse_qs(qs))
    
def parse_qs():
    qs='http://www.baidu.com/'
if __name__=='__main__':
    urlencode()