package cn.edu360;

public class Test2 {

	public static void main(String[] args) {
		//通过InnerClass.show调用的方法肯定是一个静态的方法
		InnerClass.show().test().show();
	}

}

abstract class Demo {
	public abstract void show();
}

interface Inter {
	Demo test();
}

class InnerClass {
	// 补齐代码
}